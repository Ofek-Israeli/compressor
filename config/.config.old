
#
# General
#
CONFIG_SEED=42
CONFIG_DEVICE="cuda:0"
CONFIG_OUTPUT_DIR="./outputs/phase0"
# CONFIG_LOG_LEVEL_DEBUG is not set
CONFIG_LOG_LEVEL_INFO=y
# CONFIG_LOG_LEVEL_WARNING is not set
# end of General

#
# Target (SGLang)
#
CONFIG_TARGET_MODEL_ID="meta-llama/Llama-3.1-8B-Instruct"
CONFIG_TARGET_SGLANG_BASE_URL="http://localhost:8000"
CONFIG_TARGET_SGLANG_TIMEOUT_S=120
CONFIG_TARGET_SGLANG_MAX_RETRIES=3
CONFIG_TARGET_TEMPERATURE="0.1"
CONFIG_TARGET_TOP_P="0.95"
CONFIG_TARGET_MAX_NEW_TOKENS=512
CONFIG_TARGET_SEED=42
CONFIG_TARGET_PROMPT_TEMPLATE="Context:|{context}|Question:|{query}|Answer:"
# end of Target (SGLang)

#
# Reflector (OpenAI Chat)
#
CONFIG_REFLECTOR_MODEL_ID="gpt-4o"
CONFIG_REFLECTOR_API="openai_chat_completions"
CONFIG_REFLECTOR_TEMPERATURE="0.3"
CONFIG_REFLECTOR_TOP_P="1.0"
CONFIG_REFLECTOR_MAX_NEW_TOKENS=1024
CONFIG_REFLECTOR_SEED=42
CONFIG_REFLECTOR_API_BASE="https://api.openai.com/v1"
CONFIG_REFLECTOR_API_KEY_ENV="OPENAI_API_KEY"
# end of Reflector (OpenAI Chat)

#
# Data
#
CONFIG_DATA_FINANCEBENCH_PATH="/workspace/financebench/data/financebench_open_source.jsonl"
CONFIG_DATA_SPLIT_RATIOS_TRAIN="0.70"
CONFIG_DATA_SPLIT_RATIOS_VAL="0.15"
CONFIG_DATA_SPLIT_RATIOS_TEST="0.15"
CONFIG_DATA_USE_EXPLICIT_INDICES=y
CONFIG_DATA_TRAIN_INDICES="12, 49, 100"
CONFIG_DATA_VAL_INDICES=""
CONFIG_DATA_TEST_INDICES=""
# end of Data

#
# Phase 0
#
CONFIG_PHASE0_VERBOSE_NUM_SAMPLES=1
CONFIG_PHASE0_K=75
CONFIG_PHASE0_OUTPUT_PATH="./outputs/phase0/phase0_data.json"
CONFIG_PHASE0_TARGET_CONCURRENCY=1
CONFIG_PHASE0_REFLECTOR_CONCURRENCY=4
CONFIG_PHASE0_JUDGE_CONCURRENCY=4
# end of Phase 0

#
# Phase 0 token filters
#
CONFIG_PHASE0_FILTERS_DROP_SPECIAL_TOKENS=y
CONFIG_PHASE0_FILTERS_DROP_WHITESPACE_ONLY=y
CONFIG_PHASE0_FILTERS_DROP_DIGIT_ONLY=y
# end of Phase 0 token filters

#
# Phase 0 correctness (judge)
#
CONFIG_PHASE0_CORRECTNESS_METRIC="remote_verdict"
CONFIG_PHASE0_CORRECTNESS_THRESHOLD="0.5"
CONFIG_PHASE0_CORRECTNESS_TOLERANCE="0.15"
CONFIG_PHASE0_CORRECTNESS_EVALUATOR_MODEL="gpt-4o"
CONFIG_PHASE0_CORRECTNESS_EVALUATOR_API_BASE="https://api.openai.com/v1"
CONFIG_PHASE0_CORRECTNESS_EVALUATOR_API_KEY_ENV="OPENAI_API_KEY"
CONFIG_PHASE0_CORRECTNESS_QUALITATIVE_FORGIVING=y
CONFIG_PHASE0_CORRECTNESS_EVALUATOR_TEMPERATURE="0.0"
CONFIG_PHASE0_CORRECTNESS_EVALUATOR_TOP_P="1.0"
CONFIG_PHASE0_CORRECTNESS_EVALUATOR_MAX_NEW_TOKENS=512
CONFIG_PHASE0_CORRECTNESS_EVALUATOR_SEED=42
# end of Phase 0 correctness (judge)
